print("="*10)
print("LIST")
print("="*10)

data = [1,2,3,4,5,6]
print(data)

print("menampilkan item tertentu",data[2])

data[0] = "saya"
print("setelah update",data)

# remove = menghapus item berdasarkan nama
# pop = menghapus elemen pada daftar

data.remove(6)
print("ini adalah remove",data)

data.pop(4)
print("ini adalah pop",data)

# append = menambah list dari belakan
# insert = menambah item menggunakan index

data.append("kamu")
print("ini adalah append",data)

data.insert(1,"itu")
print("ini adalah insert ",data)

for i in data:
    print(f"i adalah {i}")


